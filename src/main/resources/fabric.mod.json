{
  "schemaVersion": 1,
  "id": "twodimensional",
  "version": "${version}",
  "name": "TwoDimensional",
  "description": "Minecraft but 2d :o (Made for itsjim 2025 competition)",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/twodimensional/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "github.mishkis.twodimensional.client.TwoDimensionalClient"
    ],
    "main": [
      "github.mishkis.twodimensional.TwoDimensional"
    ]
  },
  "mixins": [
    "twodimensional.mixins.json",
    {
      "config": "twodimensional.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_3609": ["java/github/mishkis/twodimensional/utils/EntityPlaneGetterSetter"]
    }
  }
}
